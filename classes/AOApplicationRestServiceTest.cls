@IsTest
private without sharing class AOApplicationRestServiceTest {

    @IsTest
    private static void testCreateApplicationSimple() {
        // A minimalistic request with a lot of null/empty nodes to validate that the system
        // handles this gracefully.
        RestRequest request = new RestRequest();
        request.requestUri = 'https://test.salesforce.com/services/apexrest/accountopening/application';
        request.httpMethod = 'POST';
        request.requestBody = Blob.valueOf('{"form":{"name":"triage","tracking_code":"NRTL5P2","job_reference_code":"NRTL5P2","purpose":"openaccount"},"applicant":{"person":{"first_name":"Allan","last_name":"Gliege","birth_date":null,"address":null,"email":"njohnston@iccu.com","tax_id":"555-55-5555"},"persona":"Member","system_info":[]},"reports":[],"app_status":"inprogress","version":"3","application_id":null}');
        RestContext.request = request;
        RestContext.response = new RestResponse();

        Test.startTest();

        AOApplicationRestResponse response = AOApplicationRestService.createApplication();

        Test.stopTest();

        // Verify the HTTP status code.
        System.assertEquals(201, RestContext.response.statusCode);

        // Verify the response is properly formatted.
        System.assertNotEquals(null, response);
        System.assertEquals(null, response.error_messages, JSON.serialize(response.error_messages));
        System.assertNotEquals(null, response.application_id);
        System.assertEquals(36, response.application_id.length());

        // Verify that the account/contact were created properly.
        List<Contact> contacts = [
            SELECT id,
                email,
                firstName,
                lastName,
                accountId,
                account.name
            FROM Contact
        ];
        System.assertEquals(1, contacts.size());
        Contact c = contacts[0];
        System.assertEquals('njohnston@iccu.com', c.email);
        System.assertEquals('Allan', c.firstName);
        System.assertEquals('Gliege', c.lastName);
        System.assertNotEquals(null, c.accountId);
        System.assertEquals('Allan Gliege', c.account.name);

        // Verify that the account opening request was logged properly.
        List<AccountOpeningRequest__c> aoRequests = [
            SELECT id,
                applicationId__c,
                errorMessage__c,
                formName__c,
                jobReferenceCode__c,
                name,
                opportunity__c,
                processingStatus__c,
                requestData__c,
                trackingCode__c
            FROM AccountOpeningRequest__c
        ];
        System.assertEquals(1, aoRequests.size());
        AccountOpeningRequest__c aor = aoRequests[0];
        System.assertEquals(response.application_id, aor.applicationId__c);
        System.assertEquals(null, aor.errorMessage__c);
        System.assertEquals('triage', aor.formName__c);
        System.assertEquals('NRTL5P2', aor.jobReferenceCode__c);
        System.assertEquals('NRTL5P2', aor.trackingCode__c);
        System.assertEquals('NRTL5P2 - Allan Gliege', aor.name);
        System.assertNotEquals(null, aor.opportunity__c);
        System.assertEquals(AOConstants.ACCOUNT_OPENING_STATUS_COMPLETE, aor.processingStatus__c);
        System.assertEquals(request.requestBody.toString(), aor.requestData__c);

        // Verify thaat the opportunity  was created properly.
        Id recordTypeId = Schema.SObjectType.Opportunity.getRecordTypeInfosByName().get(RecordTypeConstants.OPPORTUNITY_ONLINE_ACCOUNT_OPENING).getRecordTypeId();
        List<Opportunity> opportunities = [
            SELECT id,
                accountId,
                applicationId__c,
                amount,
                aoEligibilityResponse__c,
                aoError__c,
                aoFormName__c,
                aoFormPurpose__c,
                aoFormUrl__c,
                aoResponsibleParty__c,
                app_id__c,
                avokaApplicationInfoJSON__c,
                avokaJob__c,
                avokaTrackingCode__c,
                credit_score__c,
                leadSource,
                name,
                ownerId,
                product__c,
                product_amount__c,
                productMajorCode__c,
                productMinorCode__c,
                promotionCode__c,
                recordTypeId,
                stageName,
                type
            FROM Opportunity
            WHERE id = :aor.opportunity__c
        ];
        System.assertEquals(1, opportunities.size());
        Opportunity o = opportunities[0];
        System.assertEquals(IdUtils.BATCH_USER_ID, o.ownerId);
        System.assertEquals(c.accountId, o.accountId);
        System.assertEquals(response.application_id, o.applicationId__c);
        System.assertEquals(null, o.amount);
        System.assertEquals(null, o.app_id__c);
        System.assertNotEquals(null, o.avokaApplicationInfoJSON__c);
        System.assertEquals('NRTL5P2', o.avokaJob__c);
        System.assertEquals('NRTL5P2', o.avokaTrackingCode__c);
        System.assertEquals(null, o.credit_score__c);
        System.assertEquals('Online Account Opening', o.leadSource);
        System.assertNotEquals(null, o.name);
        System.assertEquals(null, o.product__c);
        System.assertEquals(null, o.product_amount__c);
        System.assertEquals(null, o.productMajorCode__c);
        System.assertEquals(null, o.productMinorCode__c);
        System.assertEquals(null, o.promotionCode__c);
        System.assertEquals(recordTypeId, o.recordTypeId);
        System.assertEquals(OpportunityConstants.STAGE_NEW, o.stageName);
        System.assertEquals('Loan', o.type);
        System.assertEquals(null, o.aoEligibilityResponse__c);
        System.assertEquals(null, o.aoError__c);
        System.assertEquals('triage', o.aoFormName__c);
        System.assertEquals('Open Account', o.aoFormPurpose__c);
        System.assertEquals(null, o.aoFormUrl__c);
        System.assertEquals(null, o.aoResponsibleParty__c);

        // Verify that the contact role was created properly.
        List<OpportunityContactRole> roles = [
            SELECT id,
                contactId,
                isPrimary,
                role
            FROM OpportunityContactRole
            WHERE opportunityId = :aor.opportunity__c AND contactId = :c.id
        ];
        System.assertEquals(1, roles.size());
        OpportunityContactRole r = roles[0];
        System.assertEquals(true, r.isPrimary);
        System.assertEquals(AOConstants.OPPORTUNITY_ROLE_PRIMARY_BORROWER, r.role);
    }

